<%- include("partials/header") %>

<div class="container">
    <div class="row justify-ceontent-center">
        <div class="col-10 offset-lg-1 text-center">
            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Message</th> 
                    <th scope="col">Subscribe to newsletter</th> 
                    <th scope="col">Created At </th>

                  </tr>
                </thead>
                <tbody>

                  <% data.forEach(function(value, index ) { %>
                    <tr>
                        <td><%= index+1 %></td>
                        <td><%= value.name %></td>
                        <td><%= value.email %></td>
                        <td><%= value.message %></td>
                        <%if (value.checkBox > 0) { %>
                          <td>Yes</td>
                        <% } else{%>
                          <td>No</td>
                        <% } %>
                        <td><%= moment(value.created_at).format('MM-DD-YYYY HH:mm:ss') %></td>
                    </tr>
                <% }) %>
                </tbody>
              </table>
        </div>
    </div>
</div>
<%- include("partials/footer") %>